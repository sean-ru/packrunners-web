<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd" >
<module>
  <name>packrunweb</name>
  <displayName>${project.name}</displayName>
  <description>${project.description}</description>
  <versionHandler>org.packrunners.webapp.setup.PackrunnersModuleVersionHandler</versionHandler>
  <version>${project.version}</version>

  <dependencies>
    <dependency>
      <name>core</name>
      <version>${magnoliaVersion}/*</version>
    </dependency>
    <dependency>
      <name>rendering</name>
      <version>${magnoliaVersion}/*</version>
    </dependency>
    <dependency>
      <name>dam-jcr</name>
      <version>${magnoliaDamVersion}/*</version>
    </dependency>
    <dependency>
      <name>imaging</name>
      <version>2.1/*</version>
    </dependency>
    <dependency>
      <name>form</name>
      <version>2.3.3/*</version>
    </dependency>
    <dependency>
      <name>site</name>
      <version>${siteVersion}/*</version>
    </dependency>
    <dependency>
      <name>mte</name>
      <version>${mteVersion}/*</version>
    </dependency>
    <dependency>
      <name>mtk</name>
      <version>${mteVersion}/*</version>
    </dependency>
    <dependency>
      <name>multisite</name>
      <version>1.2.1/*</version>
      <optional>true</optional>
    </dependency>
    <dependency>
      <name>workflow-jbpm</name>
      <version>5.5/*</version>
      <optional>true</optional>
    </dependency>
    <dependency>
      <name>public-user-registration</name>
      <version>2.5.1/*</version>
    </dependency>

    <!-- packrunweb-pur group contains roles from these modules:  -->
    <dependency>
      <name>categorization</name>
      <version>2.4/*</version>
      <optional>true</optional>
    </dependency>
    <dependency>
      <name>contacts</name>
      <version>1.4.1/*</version>
      <optional>true</optional>
    </dependency>
    <dependency>
      <name>commenting</name>
      <version>2.3.1/*</version>
      <optional>true</optional>
    </dependency>
    <!-- packrunweb-pur group contains roles from these modules ^^^  -->

  </dependencies>
</module>
