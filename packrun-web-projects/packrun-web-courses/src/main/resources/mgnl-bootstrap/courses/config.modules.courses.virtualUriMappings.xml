<?xml version="1.0" encoding="UTF-8"?>
<sv:node sv:name="virtualUriMappings" xmlns:sv="http://www.jcp.org/jcr/sv/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <sv:property sv:name="jcr:primaryType" sv:type="Name">
    <sv:value>mgnl:content</sv:value>
  </sv:property>
  <sv:node sv:name="coursesMapping">
    <sv:property sv:name="jcr:primaryType" sv:type="Name">
      <sv:value>mgnl:contentNode</sv:value>
    </sv:property>
    <sv:property sv:name="class" sv:type="String">
      <sv:value>info.magnolia.virtualuri.mapping.RegexpVirtualUriMapping</sv:value>
    </sv:property>
    <sv:property sv:name="fromUri" sv:type="String">
      <sv:value>^/courses(.*).html</sv:value>
    </sv:property>
    <sv:property sv:name="toUri" sv:type="String">
      <sv:value>forward:/packrunweb/course?course=$1</sv:value>
    </sv:property>
  </sv:node>
</sv:node>

